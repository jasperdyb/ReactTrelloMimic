datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// 2
generator client {
  provider = "prisma-client-js"
}

// 3
model Todo {
  id          Int       @id @default(autoincrement())
  order       Int       
  createdAt   DateTime  @default(now())
  name        String
  finished    Boolean   @default(false)
}